{
  "name": "@kishieel/nestjs-schematics",
  "version": "1.0.2",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "license": "MIT",
  "schematics": "./dist/collection.json",
  "scripts": {
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "compile": "tsc --project tsconfig.json",
    "copy:schema": "cpx src/collection.json dist && cpx 'src/schematics/**/schema.json' dist/schematics",
    "copy:files": "cpx 'src/schematics/**/{files,workspace}/**/*.*' dist/schematics && cpx 'src/schematics/**/{files,workspace}/**/.!(gitignore)' dist/schematics",
    "build": "yarn compile && yarn copy:schema && yarn copy:files"
  },
  "dependencies": {
    "@angular-devkit/core": "16.2.1",
    "@angular-devkit/schematics": "16.2.1",
    "comment-json": "4.2.3",
    "jsonc-parser": "3.2.0",
    "pluralize": "8.0.0"
  },
  "devDependencies": {
    "@types/jest": "29.5.5",
    "@types/node": "18.18.4",
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "cpx": "1.5.0",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "jest": "29.7.0",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
